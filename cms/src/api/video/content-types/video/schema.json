{
  "kind": "collectionType",
  "collectionName": "videos",
  "info": {
    "singularName": "video",
    "pluralName": "videos",
    "displayName": "Video",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string"
    },
    "youtubeUrlOembed": {
      "type": "customField",
      "customField": "plugin::oembed.oembed"
    },
    "thumbnail": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "subtitle": {
      "type": "string"
    },
    "youtubeVideoId": {
      "type": "string"
    },
    "uploadDate": {
      "type": "date"
    },
    "videoType": {
      "type": "enumeration",
      "enum": [
        "Song",
        "Interview"
      ]
    },
    "videoSlug": {
      "type": "string"
    },
    "artist": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::artist.artist"
    },
    "venue": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::venue.venue"
    },
    "textYoutubeUrl": {
      "type": "string"
    }
  }
}
